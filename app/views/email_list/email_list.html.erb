<body >
	<div class = "emaillist">
		<table class = "table table-bordered">
			<tr>
				<th>
					id
				</th>
				<th>
					email
				</th>
				<th>
					Referrals
				</th>
			</tr>	
			<% @email_list.each do |email| %>
			<tr>
				<td>
					<%= email.id %>
				</td>
				<td>
					<%= email.email %>
				</td>
				<td>
					<%= email.referrals.count %>
				</td>
			</tr>
			<% end %>
		</table>
	</div>
</body>
